{
  "name": "smart-finance-tracker-backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:api": "tsx watch src/server.ts",
    "dev:worker": "tsx watch src/worker.ts",
    "dev": "concurrently -k \"npm:dev:api\" \"npm:dev:worker\"",
    "build": "tsc -p tsconfig.json",
    "start:api": "node dist/server.js",
    "start:worker": "node dist/worker.js",
    "start": "node dist/server.js",
    "seed": "node --import tsx src/seed.ts",
    "test": "vitest run"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.0",
    "@fastify/jwt": "^10.0.0",
    "@fastify/rate-limit": "^10.0.0",
    "@fastify/swagger": "^9.2.0",
    "@fastify/helmet": "^13.0.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.34.2",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "fastify": "^5.0.0",
    "fastify-plugin": "^5.0.0",
    "ioredis": "^5.4.1",
    "jose": "^5.9.0",
    "mongoose": "^8.8.4",
    "re2": "^1.21.4",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.14.15",
    "concurrently": "^9.2.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "vitest": "^4.0.16"
  }
}
